<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Messages" table="messages">
    <id name="messageId" type="integer" column="message_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="senderId" type="integer" column="sender_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="receiverId" type="integer" column="receiver_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="content" type="string" column="content" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false"/>
  </entity>
</doctrine-mapping>
